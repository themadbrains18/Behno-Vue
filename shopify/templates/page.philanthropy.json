    "philanthropy-hero": {
      "type": "philanthropy-hero",
      "blocks": {
        "e5acffd1-6c58-49d8-974e-19c2983a030e": {
          "type": "paragraph",
          "settings": {
            "para": "The National Down Syndrome Society (NDSS) and behno collaborate to create a significant partnership to further behno’s commitment to being a wholesome social enterprise founded on championing ethical manufacturing to now supporting nonprofits working to positively impact their communities."
          }
        }
      },
      "block_order": [
        "e5acffd1-6c58-49d8-974e-19c2983a030e"
      ],
      "settings": {
        "preHeading": "FRONT-END PHILANTHROPHY",
        "secHeading": "The National Down Syndrome Society x behno.",
        "image": "shopify:\/\/shop_images\/philanthropy-hero.jpg",
        "afterImageText": "WITHIN THE CAPSULE COLLECTION ARE OUR SIGNATURE BAGS, ALL IN RED TO HONOR NDSS’ OFFICIAL COLOR. 20% OF ALL SALES ARE DONATED TO NDSS."
      }
    },
    "partnership-with-ndss": {
      "type": "partnership-with-ndss",
      "settings": {
        "redText": "“WE LAUNCHED A VERY SPECIAL PARTNERSHIP WITH NDSS TO SUPPORT AND AMPLIFY THE TREMENDOUS WORK THEY DO FOR THE COMMUNITY.”",
        "secInfo": "This partnership is very personal to us. The Nini Bag, the jewel of this capsule collection, pays homage to behno’s founder’s darling sister, Nirali, nicknamed “Nini,” who has Down Syndrome.",
        "checkImage": true,
        "videolink": "https:\/\/cdn.shopify.com\/s\/files\/1\/1000\/3130\/files\/Full-Length-behno-on-the-Road.mp4?v=1651818176"
      }
    },
