<render-sense-of-behno-behno-in-world-speaking-engagement :shopify-data="{
    secHeading: `{{ section.settings.secHeading }}`,
    secSubHeading: `{{ section.settings.secSubHeading }}`,
    secInfo: `{{ section.settings.secInfo }}`,
    InfoBreak: `{{ section.settings.InfoBreak }}`,
    imgUrl: {
        src : `{{ section.settings.secImg | img_url : "" }}`,
        placeholder: `{{ section.settings.secImg | img_url : "50x50" }}`,
        alt: `{{ section.settings.secImg.alt }}`
    }

}">
</render-sense-of-behno-behno-in-world-speaking-engagement>


{% schema %}
{
    "name":"Speaking Engagement",
    "settings":[
        {
            "id":"secHeading",
            "type":"text",
            "label":"Section Heading",
            "default":"Speaking engagement at the United Nations, a first of its kind."
        },
        {
            "id":"secSubHeading",
            "type":"text",
            "label":"Section Sub Heading",
            "default":"behno founder, Shivam Punjya, was invited to the United Nations to speak on fashion as a social mobilizer."

        },
        {
            "id":"secInfo",
            "type":"text",
            "label":"Section Info",
            "default":"behno founder spoke on fashion as an industry capable of mobilizing communities and consumers for social change and global impact and share his story and social enterprise model. The talk was attended by diplomats, figureheads, and industry veterans."

        },
        {
            "id":"InfoBreak",
            "type":"text",
            "label":"Section Info After Break",
            "default":"For the first time, the United Nations discussed the fashion industry as a mobilizer for change by inviting six leaders in the fashion space to present their business models to discuss best practices and stimulate dialogue."

        },
        {
            "id":"secImg",
            "type":"image_picker",
            "label":"Section Image"
        }
    ]
}
{% endschema %}
