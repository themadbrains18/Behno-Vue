{% capture paraData %}
[
   {% for block in section.blocks %}
        {% if block.type == 'paragraph' %}
            {
                para : `{{ block.settings.para }}`
            }{% unless forloop.last %},{% endunless %}
        {% endif %}
   {% endfor %}
]
{% endcapture %}

<render-sense-of-behno-our-partner-handbag-Urbanism-and-development :shopify-data="{
    secHeading:`{{ section.settings.secHeading }}`,
    secSubHeading:`{{ section.settings.subHeading }}`,
    paragraph  :  {{ paraData }},
    SecImg : {
        src : `{{ section.settings.secImg | img_url : "" }}`,
        placeholder: `{{ section.settings.secImg | img_url : "50x50" }}`,
        alt: `{{ section.settings.secImg.alt }}`
    },
}">
</render-sense-of-behno-our-partner-handbag-Urbanism-and-development>


